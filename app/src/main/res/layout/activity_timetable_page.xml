<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ttbg"
    tools:context=".timetablePage">

    <TextView
        android:id="@+id/textView14"
        android:layout_width="225dp"
        android:layout_height="36dp"
        android:layout_marginTop="28dp"
        android:text="Add New Timetable?"
        android:textAlignment="center"
        android:textColor="#4CAF50"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView44"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="29dp"
        android:layout_height="28dp"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/schoolicon"
        tools:layout_editor_absoluteX="211dp" />

    <ImageView
        android:id="@+id/imageView44"
        android:layout_width="14dp"
        android:layout_height="42dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pole" />

    <ImageView
        android:id="@+id/imageView51"
        android:layout_width="50dp"
        android:layout_height="26dp"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/namelogo"
        tools:layout_editor_absoluteX="147dp" />

    <ImageView
        android:id="@+id/imageView53"
        android:layout_width="338dp"
        android:layout_height="450dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:srcCompat="@drawable/ttpic" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="140dp"
        android:layout_height="128dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView53"
        app:srcCompat="@drawable/addtt" />
</androidx.constraintlayout.widget.ConstraintLayout>