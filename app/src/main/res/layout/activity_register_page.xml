<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterPage">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="480dp"
        android:layout_height="572dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/purplewave"
        tools:layout_editor_absoluteY="76dp" />

    <EditText
        android:id="@+id/emailRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/fillbox"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="Email...."
        android:textAlignment="center"
        android:textColor="#4A4DFF"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="153dp" />

    <Button
        android:id="@+id/signup_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button"
        android:backgroundTint="#4A4DFF"
        android:text="Sign Up!"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="442dp" />

    <EditText
        android:id="@+id/pwdRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/fillbox"
        android:ems="10"
        android:inputType="textPassword"
        android:text="Password"
        android:textAlignment="center"
        android:textColor="#4A4DFF"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="208dp" />

    <EditText
        android:id="@+id/firstnRegister"
        android:layout_width="109dp"
        android:layout_height="37dp"
        android:background="@drawable/fillbox"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="First Name"
        android:textAlignment="center"
        android:textColor="#4A4DFF"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="260dp" />

    <EditText
        android:id="@+id/courseRegister"
        android:layout_width="240dp"
        android:layout_height="47dp"
        android:background="@drawable/fillbox"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Course (CS270)"
        android:textAlignment="center"
        android:textColor="#4A4DFF"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="311dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="Register"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="87dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="14dp"
        android:layout_height="42dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/pole"
        tools:layout_editor_absoluteY="20dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="50dp"
        android:layout_height="26dp"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/namelogo"
        tools:layout_editor_absoluteX="147dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="29dp"
        android:layout_height="28dp"
        app:srcCompat="@drawable/schoolicon"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="27dp" />

    <EditText
        android:id="@+id/secondnRegister"
        android:layout_width="109dp"
        android:layout_height="37dp"
        android:background="@drawable/fillbox"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Last Name"
        android:textAlignment="center"
        android:textColor="#4A4DFF"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="260dp" />

</androidx.constraintlayout.widget.ConstraintLayout>