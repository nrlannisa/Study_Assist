<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".landingPage">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:srcCompat="@drawable/schoolicon"
        tools:layout_editor_absoluteY="67dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="110dp"
        android:layout_height="75dp"
        android:layout_marginTop="72dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/namelogo"
        tools:layout_editor_absoluteX="85dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="14dp"
        android:layout_height="123dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:srcCompat="@drawable/pole"
        tools:layout_editor_absoluteY="46dp" />

    <ProgressBar
        android:id="@+id/testBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="#D3E6A5"
        android:indeterminateDrawable="@drawable/proggreen_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.182"
        app:layout_constraintStart_toEndOf="@+id/taskBar"
        tools:layout_editor_absoluteY="207dp" />

    <AnalogClock
        android:id="@+id/simpleAnalogClock"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:background="#D3D0D0"
        android:padding="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/testBar" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time"
        android:textColor="#000000"
        android:textSize="8sp"
        tools:layout_editor_absoluteX="197dp"
        tools:layout_editor_absoluteY="377dp" />

    <ProgressBar
        android:id="@+id/taskBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="60dp"
        android:background="#A5A6F6"
        android:indeterminateDrawable="@drawable/progpurple_bg"
        app:layout_constraintTop_toBottomOf="@+id/imageView14"
        tools:layout_editor_absoluteX="105dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Task"
        android:textSize="8sp"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="253dp" />

    <TextView
        android:id="@+id/tvcounter"
        android:layout_width="39dp"
        android:layout_height="37dp"
        android:fontFamily="@font/poppins_bold"
        android:text="3/7"
        android:textAlignment="center"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomof="parent"
        app:layout_constraintRight_toRightof="parent"
        app:layout_constraintTop_toTopof="parent"
        app:layout_constraintleft_toleftof="parent"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="238dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="39dp"
        android:layout_height="37dp"
        android:fontFamily="@font/poppins_bold"
        android:text="2/4"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="238dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="38dp"
        android:layout_height="12dp"
        android:text="Final/Test"
        android:textSize="8sp"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="253dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button2"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/round_btn1"
        android:fontFamily="@font/poppins_bold"
        android:text="Task"
        android:textColor="#FFFFFF"
        android:viewportWidth="65"
        android:viewportHeight="38"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/simpleAnalogClock" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button3"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:background="#5C5C5C"
        android:background="@drawable/round_btn3"
        android:fontFamily="@font/poppins_bold"
        android:text="Courses"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button4"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:background="#719F00"
        android:background="@drawable/round_btn2"
        android:fontFamily="@font/poppins_bold"
        android:text="Upcoming Paper"
        android:textColor="#FFFFFF"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        tools:layout_editor_absoluteX="124dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="#D3E6A5"
        android:background="@drawable/round_btn4"
        android:fontFamily="@font/poppins_bold"
        android:text="Timetable"
        android:textColor="#FFFFFF"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:layout_editor_absoluteX="107dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="#A5A6F6"
        android:background="@drawable/round_btn5"
        android:fontFamily="@font/poppins_bold"
        android:text="My Profile"
        android:textColor="#FFFFFF"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:layout_editor_absoluteX="218dp" />

</androidx.constraintlayout.widget.ConstraintLayout>